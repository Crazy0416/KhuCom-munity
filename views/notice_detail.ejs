<div class="the-box no-border blog-detail-content">
  <blockquote>
    <h3><%= post.post_title%></h3>
    <small>
      <table>
        <tr><td><i class="fa fa-calendar icons"></i>
          Posted : <a href=""><%=post.post_register_datetime%></a> </td></tr>
        <tr><td><i class="fa fa-flask icons"></i>
        Author: <a href=""><%=post.post_username%></a></td></tr>
      </table>
    </small>
  </blockquote>
  <p class="text-justify">
      <%-post.post_content%>
  </p>
  <hr />
</div><!-- /the.box .no-border -->
